{
  "name": "kindundnatur",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config-develop": "ts-node ./scripts/setenv.ts --environment=develop",
    "config-prod": "ts-node ./scripts/setenv.ts --environment=prod",
    "start": "npm run config-develop --environment=develop && ng serve --configuration=custom",
    "build": "npm run config-prod --environment=prod && ng build --configuration=custom",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run kindundnatur:serve-ssr",
    "serve:ssr": "node dist/kindundnatur/server/main.js",
    "build:ssr": "ng build && ng run kindundnatur:server",
    "prerender": "ng run kindundnatur:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/platform-server": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@angular/ssr": "^17.0.0",
    "@netlify/functions": "^1.4.0",
    "express": "^4.18.2",
    "netlify-cli": "^17.2.2",
    "rxjs": "~7.4.0",
    "swiper": "^11.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^18.18.9",
    "dotenv": "^16.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "node-fetch": "^3.3.1",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "typescript": "~5.2.2",
    "yargs": "^17.4.0"
  }
}
