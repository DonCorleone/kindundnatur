{
  "name": "kindundnatur",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config-develop": "ts-node ./scripts/setenv.ts --environment=develop",
    "config-prod": "ts-node ./scripts/setenv.ts --environment=prod",
    "start": "npm run config-develop --environment=develop && ng serve --configuration=custom",
    "build": "npm run config-prod --environment=prod && ng build --configuration=custom",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.0",
    "ngx-useful-swiper": "^10.0.1",
    "rxjs": "~7.4.0",
    "swiper": "^6.8.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.55",
    "dotenv": "^16.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "typescript": "~4.8.4",
    "yargs": "^17.4.0"
  }
}